import java.util.ArrayList;
public class Solution {
    static boolean searchMatrix(ArrayList<ArrayList<Integer>> mat, int target) {
        for(int i=0;i<mat.size();i++){
            for(int j=0;j<mat.get(0).size();j++){
                if(mat.get(i).get(j)==target){
                    return true;
                }
            }
        }
        return false;
    }
}
